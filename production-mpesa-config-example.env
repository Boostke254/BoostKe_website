# Production M-Pesa Configuration Example
MPESA_ENVIRONMENT=production

# Replace with your actual production credentials
MPESA_CONSUMER_KEY=your_production_consumer_key
MPESA_CONSUMER_SECRET=your_production_consumer_secret
MPESA_BUSINESS_SHORT_CODE=your_production_paybill_number
MPESA_PASSKEY=your_production_passkey

# Update callback URLs to your production domain
MPESA_CALLBACK_URL=https://yourdomain.com/api/mpesa/callback
MPESA_TIMEOUT_URL=https://yourdomain.com/api/mpesa/timeout
